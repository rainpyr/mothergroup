<%= form_with model: @activity, local: true, html: { mulipart: true } do |f| %>

<%= f.label :title%>
<br>
<%= f.text_field :title %>
<br><br>

<%= f.label :in_location %>
<br>
<%= f.text_field :in_location %>
<br><br>

<%= f.label :takeplace_time %>
<br>
<%= f.datetime_field :takeplace_time %>
<br><br>

<%= f.label :description %>
<br>
<%= f.text_field :description %>
<br><br>

<%= f.label :category_id, "Categories"%>
<br>
<%= f.collection_select :category_id, Category.all, :id, :name, include_blank: true, multiple: true %>
<br><br>

<%= f.label :image %>
<br>
<%= f.file_field :image %>
<br><br>

<%= f.submit %>

<% end %>